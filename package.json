{
  "name": "@faire/babel-plugin-formatjs-localized-bundle",
  "version": "2.0.0",
  "main": "dist/index.js",
  "author": "Paul Salvatore <paul@faire.com>",
  "license": "MIT",
  "description": "Inline translations into formatjs code at compile time",
  "bin": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Faire/babel-plugin-formatjs-localized-bundle.git"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-core": "^6.26.3",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.5",
    "npm-run-all": "^4.1.5",
    "husky": "^6.0.0",
    "jest": "^28.1.3",
    "pretty-quick": "^3.1.0"
  },
  "scripts": {
    "test": "jest ./src/",
    "test:ci": "jest --ci",
    "clean": "rm -rf dist",
    "build": "rsync -av --progress . ./dist --exclude dist",
    "generatePackageJson": "node scripts/generatePackageJson.js",
    "publishProject": "node scripts/publishProject.js",
    "publishNewVersion": "npm-run-all clean build generatePackageJson publishProject"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
